//comando switch

let numero = Number(prompt("Digite um número de 1 a 4 inteiro"));

switch (numero) {
  case 1 :
    console.log("Um");
  break;
   case 2 :
    console.log("Dois");
  break;
  case 3 :
    console.log("Três");
  break;
  case 4 :
    console.log("Quatro");
  break;
  default:
    console.log("Este número não está na faixa");  
}