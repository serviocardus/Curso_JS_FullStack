let arr = []; cont = 0;

for (let i = 0; i <= 4; i++){
  
  //Infomando os números para as 5 primeiras posições do vetor
  arr[i] = Number(prompt(`Infome um número real para a posiçao ${i} : `));
   
}

cont = arr.length;
//console.log(cont);

for (let j = 4; j >= 0; j--){
  arr[cont] = arr[j];
  cont++;
}

console.log(arr);

//Hoisting - reconhece uma função mesmo ela sendo declarada após sua chamada
console.log(mensagem());

function mensagem(){
  
   return "estou aqui!!!";
 }

 //Retornando os argumentos de uma função
 function func(){
  console.log(arguments);
}

func(1,2,3,4,5,6);
